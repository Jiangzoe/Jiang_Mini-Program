page{
    width: 100%;
    height: 100vh;
    background-color: #f4f6f8;
}
.section{
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    position: fixed;
    left: 0;
    top: 0;
}
.nav{
    height: 108rpx;
    width: 550rpx;
    line-height: 108rpx;
    box-sizing: border-box;
    position: fixed;
    overflow: hidden;
    font-size: 16px;
    white-space: nowrap;
    top: 0;
    left: 100rpx;
    right: 100rpx;
    z-index: 999;
    margin-bottom: 32rpx;
}
.nav-all{
    height: 30rpx;
    width: 30rpx;
    position: absolute;
    right: 0;
    top: 0;
    transform: scale(0.18);
    margin-right: 80rpx;
}
.nav-me{
    width: 30rpx;
    height: 30rpx;
    left: 0;
    margin-top: 10rpx;
    position: absolute;
    transform: scale(.15)
}
.nav-item{
    margin-left: 20rpx;
    margin-right: 20rpx;
    display: inline-block;
    text-align: center;
}
.nav-item.active{
    color:#fc975c;
}
.content{
    position: fixed;
    width: 686rpx;
    height: 438rpx;
    background-color: #fff;
    left: 0;
    right: 0;
    margin-left: auto;
    margin-right: auto;
    top: 140rpx;
    box-shadow: 1px 1px 6px #f4f6f8;
}
.container .didi-time{
    position: absolute;
    width: 100%;
    height: 92rpx;
    left: 40rpx;
    top: 45rpx;
    font-size: 30rpx;
    color: rgba(0,0,0,.7);
}
.didi-input-str{
    position: absolute;
    top: 100rpx;
    left: 105rpx;
    border-bottom: 1px solid #f5f5f5;
    height: 120rpx;
    width: 100%;
    line-height: 120rpx;
}
.circle-str{
    position: absolute;
    height: 10px;
    width: 10px;
    border-radius: 50%;
    background: #3cbca3;
    left: 40rpx;
    top: 150rpx;
}
.didi-input-end{
    position: absolute;
    height: 120rpx;
    width: 100%;
    margin-top: -300rpx;
    line-height: 120rpx;
    left: 105rpx;
  }
  
  .circle-end{
    position: absolute;
    height: 10px;
    width: 10px;
    border-radius: 50%;
    background: #fc9153;
    left: 40rpx;
    top: 250rpx;
  }
  .container .didi-btn{
    position: absolute;
    width: 100%;
    height: 100rpx;
    background-color: #4a4c5b;
    bottom: 0;
    color: #fff;
    left: 0;
}